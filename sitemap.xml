---
layout: null
searchable: false
sitemap:
  exclude: true
---

<?xml version="1.0" encoding="utf-8"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd"
        xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {%- assign pages = site.pages | site.webpages | concat: site.archives -%}
    {% for page in pages %}
        {% unless page.draft or page.title == null or page.title == empty or page.sitemap.exclude == true %}
            <url>
                <loc>{{ page.url | absolute_url }}</loc>
                <lastmod>{{ page.sitemap.lastmod | default: page.date | default: site.time | date_to_xmlschema }}</lastmod>
                <changefreq>{{ page.sitemap.changefreq | default: site.sitemap.changefreq }}</changefreq>
                <priority>{{ page.sitemap.priority | default: site.sitemap.priority }}</priority>
            </url>
        {% endunless %}
    {% endfor %}
    {% for page in site.posts %}
        {% unless page.draft or page.sitemap.exclude == true %}
            <url>
                <loc>{{ page.url | absolute_url }}</loc>
                <lastmod>{{ page.sitemap.lastmod | default: page.date | default: site.time | date_to_xmlschema }}</lastmod>
                <changefreq>{{ page.sitemap.changefreq | default: site.sitemap.changefreq }}</changefreq>
                <priority>{{ page.sitemap.priority | default: site.sitemap.priority }}</priority>
            </url>
        {% endunless %}
    {% endfor %}
</urlset>
