<h2>Browse by category</h2>
<ul class=" [ cluster ] ">
	{% for lookup in collections.categories -%}
		{%- if page.url != '/' + lookup.plural + '/' %}<li><a href="/{{ lookup.plural }}/" {% if category == lookup.title %}class=" [ button  button--text ] "{% endif %}>{{ lookup.properPlural | title }}</a></li>{% endif -%}
	{%- endfor %}
</ul>
