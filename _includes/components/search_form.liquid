<form class="search-form  js-search-form" id="search" name="search" method="get" action="/search" role="search">
    <label class="search-label" id="search-label" for="search-input" hidden>Search for pages and posts</label>
    <input type="search" inputmode="search" class="search-input  js-search-input" id="search-input" name="query" value="" list="search-types" maxlength="30" size="1" aria-labelledby="search-label" aria-required="true" required aria-disabled="true" disabled>
    <datalist id="search-types">
        <option value="date:" label="Date Search">
        <option value="tag:" label="Tag Search">
    </datalist>
    <input type="submit" role="button" class="button  search-submit  js-search-submit" id="search-button" value="Search" aria-label="Search" aria-disabled="true" disabled>
</form>
<noscript>Please enable JavaScript to search.</noscript>
